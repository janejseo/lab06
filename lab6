import java.util.Scanner;

public class lab06 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int num1, num2;

        System.out.print("Enter a number: ");
        num1 = input.nextInt();

        System.out.print("Enter another number: ");
        num2 = input.nextInt();
        
        int sum;
        sum = num1 - num2;

        System.out.println(sum);
    }
}
